@{
    ViewData["Title"] = "Home Page";
}


@model User
<h2> Enter Your information</h2>
<form action="/Success" method="POST">
    <div class="form-group">
        <span asp-validation-for="@Model.FirstName"></span>
        <label asp-for="@Model.FirstName"></label>
        <input type="text" asp-for="@Model.FirstName" >
    </div>
    <div class="form-group">
        <span asp-validation-for="@Model.LastName"></span>
        <label asp-for="@Model.LastName"></label>
        <input type="text" asp-for="@Model.LastName" >
    </div>
    <div class="form-group">
        <span asp-validation-for="@Model.Age"></span>
        <label asp-for="@Model.Age"></label>
        <input type="text" asp-for="@Model.Age" >
    </div>
    <div class="form-group">
        <span asp-validation-for="@Model.Email"></span>
        <label asp-for="@Model.Email"></label>
        <input type="text" asp-for="@Model.Email" >
    </div>
    <div class="form-group">
        <span asp-validation-for="@Model.Password"></span>
        <label asp-for="@Model.Password"></label>
        <input type="password" asp-for="@Model.Password" >
    </div>
    <input type="submit" value="Register" >
</form>